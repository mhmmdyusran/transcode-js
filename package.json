{
  "name": "transcode-js",
  "version": "1.0.0",
  "description": "Production-grade stateless format conversion library. Transform data formats with a single function call.",
  "author": "mhmmdyusran",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "transcode",
    "transcoder",
    "format",
    "converter",
    "transformer",
    "json",
    "yaml",
    "xml",
    "toml",
    "csv",
    "bson",
    "msgpack",
    "cbor",
    "ini",
    "ndjson",
    "json5",
    "hjson",
    "urlencoded"
  ],
  "license": "MIT",
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "bson": "^6.10.3",
    "cbor": "^9.0.2",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.5.2",
    "fast-xml-parser": "^4.5.1",
    "hjson": "^3.2.2",
    "ini": "^5.0.0",
    "json5": "^2.2.3",
    "msgpackr": "^1.11.2",
    "qs": "^6.13.1",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@types/hjson": "^2.4.6",
    "@types/ini": "^4.1.1",
    "@types/node": "^22.12.0",
    "@types/qs": "^6.9.17",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  }
}
